{
  "dashboard": {
    "title": "xApp Performance Dashboard",
    "tags": ["ntn", "xapp", "performance"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "type": "graph",
        "title": "Handover xApp - Prediction Accuracy",
        "targets": [
          {
            "expr": "handover_prediction_accuracy_percent",
            "legendFormat": "Prediction Accuracy",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Accuracy", "min": 0, "max": 100},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [85],
                "type": "lt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "Low Handover Prediction Accuracy",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 2,
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "type": "graph",
        "title": "Handover xApp - Decision Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(handover_decision_latency_ms_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(handover_decision_latency_ms_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(handover_decision_latency_ms_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Latency"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 3,
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "type": "graph",
        "title": "Power xApp - Power Consumption Optimization",
        "targets": [
          {
            "expr": "power_consumption_watts",
            "legendFormat": "Current Power Consumption",
            "refId": "A"
          },
          {
            "expr": "power_target_watts",
            "legendFormat": "Target Power",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "watt", "label": "Power"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 4,
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "type": "graph",
        "title": "Power xApp - RSRP Maintenance",
        "targets": [
          {
            "expr": "avg(power_xapp_rsrp_dbm)",
            "legendFormat": "Average RSRP",
            "refId": "A"
          },
          {
            "expr": "power_xapp_rsrp_target_dbm",
            "legendFormat": "Target RSRP",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "dBm", "label": "RSRP"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 5,
        "gridPos": {"x": 0, "y": 16, "w": 8, "h": 6},
        "type": "stat",
        "title": "Handover Success Rate",
        "targets": [
          {
            "expr": "rate(handover_success_total[5m]) / (rate(handover_success_total[5m]) + rate(handover_failure_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "value_and_name",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "gridPos": {"x": 8, "y": 16, "w": 8, "h": 6},
        "type": "stat",
        "title": "Power Adjustments per Minute",
        "targets": [
          {
            "expr": "rate(power_adjustments_total[1m]) * 60",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "value_and_name",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "adjustments/min",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 50, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "gridPos": {"x": 16, "y": 16, "w": 8, "h": 6},
        "type": "stat",
        "title": "xApp Processing Time (avg)",
        "targets": [
          {
            "expr": "avg(xapp_processing_time_ms)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "value_and_name",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 20, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "gridPos": {"x": 0, "y": 22, "w": 12, "h": 6},
        "type": "graph",
        "title": "xApp Request Rate",
        "targets": [
          {
            "expr": "rate(xapp_requests_total{xapp=\"handover\"}[1m])",
            "legendFormat": "Handover xApp",
            "refId": "A"
          },
          {
            "expr": "rate(xapp_requests_total{xapp=\"power\"}[1m])",
            "legendFormat": "Power xApp",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "req/s", "label": "Request Rate"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 9,
        "gridPos": {"x": 12, "y": 22, "w": 12, "h": 6},
        "type": "graph",
        "title": "xApp Error Rate",
        "targets": [
          {
            "expr": "rate(xapp_errors_total{xapp=\"handover\"}[5m])",
            "legendFormat": "Handover xApp Errors",
            "refId": "A"
          },
          {
            "expr": "rate(xapp_errors_total{xapp=\"power\"}[5m])",
            "legendFormat": "Power xApp Errors",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "errors/s", "label": "Error Rate"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}
